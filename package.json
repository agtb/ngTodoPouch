{
  "name": "todo-list",
  "version": "0.0.0",
  "dependencies": {
    "express": "^4.3.2",
    "nano": "^5.9.1"
  },
  "devDependencies": {
    "tap": "^0.4.9",
    "nodemon": "^1.1.1",
    "handlebars": "^1.3.0",
    "hbsfy": "^1.3.2",
    "atomify": "^2.2.2",
    "jshint": "^2.5.1"
  },
  "scripts": {
    "watch": "atomify -d",
    "start": "nodemon server.js",
    "test": "tap **/*_test.js",
    "dev": "npm run watch & npm start"
  },
  "atomify": {
    "js": {
      "entry": "app/index.js",
      "output": "www/bundle.js"
    },
    "css": {
      "entry": "app/index.css",
      "output": "www/bundle.css"
    }
  }
}
